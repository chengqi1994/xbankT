<ehcache>
  <diskStore path="java.io.tmpdir"/>

  <defaultCache
  maxElementsInMemory="200000"
  eternal="true"
  timeToIdleSeconds="0"
  timeToLiveSeconds="0"
  overflowToDisk="true"
  />

  <!--StructUserInfo的查询缓存配置-->
  <cache name="StructUserInfo"
  maxElementsInMemory="400000"
  eternal="true"
  timeToIdleSeconds="0"
  timeToLiveSeconds="0"
  overflowToDisk="true"
  />
  
  
</ehcache>
